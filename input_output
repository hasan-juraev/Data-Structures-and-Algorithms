#include <iostream>
using namespace std;

int main()
{
    int marks[5] = {0};
    
    int n;
    cout << "Enter the number of students ";
    cin >> n;
    
    // Input
    for(int i=1; i<=n; i++) {
        cin>> marks[i];
        
        marks[i] = marks[i] * 2;
    }
    
    //Output
    for(int i=0; i<5; i++) {
        cout << marks[i] << " ,";
    }
    cout << endl;

    return 0;
}
